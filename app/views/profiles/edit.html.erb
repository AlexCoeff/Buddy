<div class="container">
<h1>Update your profile</h1>

<%= simple_form_for(current_user, url: profiles_path) do |f| %>
  <%= f.input :first_name %>
  <%= f.input :last_name %>
  <%= f.input :description %>
  <%= f.input :background %>
  <%= f.input :values %>
  <%= f.submit class: "btn btn-primary " %>
<% end %>

</div>
